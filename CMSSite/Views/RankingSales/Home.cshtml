@{ 
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport"
          content="width=device-width,  initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <title>BYTESOFT</title>
    <link rel="icon" href="~/RankingSale/images/icon/fav_icon.png" type="image/png" />
    <link rel="stylesheet" type="text/css" href="~/RankingSale/css/tool.min.css" />
    <link rel="stylesheet" type="text/css" href="~/RankingSale/css/main.min.css" />

</head>
<body class="index">
    <div class="main-web">
        <div class="overlay"></div>
        <div class="main-right">
            <div id="header">
                <div class="bs-container header-container">
                    <div class="header-left">
                        <a href="/">
                            <img src="~/RankingSale/images/layout/logo.png"
                                 width="167"
                                 height="37"
                                 alt="logo"
                                 class="logo" />
                        </a>
                    </div>
                    <div class="header-right">
                        <div class="right-link active" data-aos="fade-down">
                            <a href="/">trang chủ</a>
                        </div>
                        <div class="right-link" data-aos="fade-down" data-aos-delay="100">
                            <a href="/">bảng xếp hạng</a>
                        </div>
                        <div class="right-link" data-aos="fade-down" data-aos-delay="200">
                            <a href="/history.html">lịch sử</a>
                        </div>
                    </div>

                    <img class="open-menu"
                         alt="open"
                         src="~/RankingSale/images/layout/bars.svg"
                         width="30"
                         height="30" />
                </div>

                <div class="menu-side">
                    <img class="close-menu"
                         alt="close"
                         src="~/RankingSale/images/layout/window-close.svg"
                         width="30"
                         height="30" />
                    <div class="right-link active">
                        <a href="/">trang chủ</a>
                    </div>
                    <div class="right-link">
                        <a href="/">bảng xếp hạng</a>
                    </div>
                    <div class="right-link">
                        <a href="/history.html">lịch sử</a>
                    </div>
                </div>
            </div>

            <img class="to-top" src="~/RankingSale/images/layout/caret-square-up.svg" alt="to top" width="40" height="40" />
            <div id="main">
                <div class="main-content">
                    <section class="section_index">
                        <!-- sale rank -->
                        <div class="module module-sale_rank">
                            <div class="grid">
                                <img src="~/RankingSale/images/index/grid.png" alt="grid" />
                            </div>
                            <div class="module-header" data-aos="fade-down">
                                <h2 class="header">bảng xếp hạng sale</h2>
                                <p class="sub-header">ngày 12 tháng 07 năm 2021</p>
                            </div>
                            <div class="module-content">
                                <div class="ranks">
                                    <!-- 2nd -->
                                    <div class="_2nd rank-item" data-aos="fade-right" data-aos-delay="500">
                                        <!-- avt -->
                                        <div class="avt" data-rank="2">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/avatar.png" alt="avatar" />
                                            </div>

                                            <div class="honor">
                                                <img src="~/RankingSale/images/index/honor_2nd.png" alt="honor" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-to_right.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">2nd</span>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- 1st -->
                                    <div class="_1st rank-item" data-aos="zoom-in" data-aos-delay="200">
                                        <div class="avt" data-rank="1">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/avatar.png" alt="avatar" />
                                            </div>
                                            <div class="honor">
                                                <img src="~/RankingSale/images/index/honor_1st.png"
                                                     alt="honor"
                                                     width="273"
                                                     height="220" />
                                            </div>
                                            <div class="crown">
                                                <img src="~/RankingSale/images/index/crown.png" alt="crowm" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-center.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">WINNER</span>
                                            </div>
                                        </div>

                                        <img src="~/RankingSale/images/index/vs.png"
                                             alt="vs"
                                             class="vs _1"
                                             width="50"
                                             height="24" />
                                        <img src="~/RankingSale/images/index/vs.png"
                                             alt="vs"
                                             class="vs _2"
                                             width="50"
                                             height="24" />
                                    </div>

                                    <!-- 3rd -->
                                    <div class="_3rd rank-item" data-aos="fade-left" data-aos-delay="500">
                                        <div class="avt" data-rank="3">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/avatar.png" alt="avatar" />
                                            </div>
                                            <div class="honor">
                                                <img src="~/RankingSale/images/index/honor_2nd.png" alt="honor" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-to_left.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">3nd</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- week rank -->
                        <div class="module module-week_rank">
                            <div class="module-header" data-aos="fade-down">
                                <h3 class="header">Bảng xếp hạng tuần</h3>
                                <p class="sub-header">Tuần 2 tháng 07 năm 2021</p>
                            </div>
                            <div class="module-content">
                                <div class="ranks">
                                    <!-- 2nd -->
                                    <div class="rank-item _2nd" data-aos="fade-right" data-aos-delay="200">
                                        <div class="avt" data-rank="2">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/week_rank-avt-2.png" alt="avt" />
                                                <div class="border-outer"></div>
                                                <div class="border-inner"></div>
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-to_right.png" alt="info" />
