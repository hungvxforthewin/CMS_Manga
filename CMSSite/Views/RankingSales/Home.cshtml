                                    <div class="rank-item _2nd _2nd_1st" data-aos="fade-right" data-aos-delay="100">
                                        <div class="avt" data-rank="1">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/week_rank-avt-2.png" alt="avt" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-to_right.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">2nd</span>
                                            </div>
                                        </div>
                                        <img src="~/RankingSale/images/index/vs-2.png"
                                             alt="vs"
                                             class="vs _1"
                                             width="88"
                                             height="50" />
                                        <img src="~/RankingSale/images/index/vs-2.png"
                                             alt="vs"
                                             class="vs _2"
                                             width="88"
                                             height="50" />
                                    </div>

                                    <!-- 3rd -->
                                    <div class="rank-item _2nd" data-aos="fade-right">
                                        <div class="avt" data-rank="3">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/week_rank-avt-2.png" alt="avt" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-to_right.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">2nd</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- month rank -->
                        <div class="module module-month_rank" data-aos="fade-down">
                            <div class="grid">
                                <img src="~/RankingSale/images/index/grid-2.png" alt="grid" />
                            </div>

                            <div class="module-header">
                                <h3 class="header">bảng xếp hạng tháng</h3>
                                <p class="sub-header">tháng 07 năm 2021</p>
                            </div>

                            <div class="module-content">
                                <div class="ranks">
                                    <!-- 2nd -->
                                    <div class="rank-item _2nd" data-aos="fade-right" data-aos-delay="500">
                                        <!-- avt -->
                                        <div class="avt" data-rank="2">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/month_rank-avt.png" alt="avt" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-to_right.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">3nd</span>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- 1st -->
                                    <div class="rank-item _1st" data-aos="zoom-in" data-aos-delay="200">
                                        <!-- avt -->
                                        <div class="avt" data-rank="1">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/month_rank-avt.png" alt="avt" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-center.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">WINNER</span>
                                            </div>
                                        </div>
                                        <img src="~/RankingSale/images/index/vs-2.png"
                                             alt="vs"
                                             class="vs _1"
                                             width="88"
                                             height="50" />
                                        <img src="~/RankingSale/images/index/vs-2.png"
                                             alt="vs"
                                             class="vs _2"
                                             width="88"
                                             height="50" />
                                    </div>

                                    <!-- 3rd -->
                                    <div class="rank-item _3rd" data-aos="fade-left" data-aos-delay="500">
                                        <!-- avt -->
                                        <div class="avt" data-rank="3">
                                            <div class="avt-container">
                                                <img src="~/RankingSale/images/index/month_rank-avt.png" alt="avt" />
                                            </div>
                                        </div>
                                        <!-- detail -->
                                        <div class="info">
                                            <img src="~/RankingSale/images/index/detail-to_left.png" alt="info" />
                                            <div class="detail">
                                                <p class="name">Nguyễn thị minh anh</p>
                                                <p class="address">Phòng Sale: 02</p>
                                                <span class="rank">3nd</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </section>
                </div>
            </div>
            <div id="footer">
                <!-- partners -->
                <div class="module-partners">
                    <div class="bs-slide partner-slide">

                        <div class="partner-item item">
                            <img src="~/RankingSale/images/layout/logo.png" alt="partner" />
                        </div>

                        <div class="partner-item item">
                            <img src="~/RankingSale/images/layout/logo.png" alt="partner" />
