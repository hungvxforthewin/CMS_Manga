<div class="pull-right">
    <a class="hidden-sm hidden-xs" href="~/Templates/Import/NS.xlsx">>>Tải Excel Mẫu</a>
</div>
<form class="form-inline">
    <input name="fileImportExcel" id="fileImportExcelCustomer" type="file" style="display:none" accept=".csv, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
</form>
<button class="btn btn-primary hidden-sm hidden-xs left5" id="btnImportExcel" type="button"><i class="fa fa-upload" aria-hidden="true"></i><span class="hidden-sm hidden-xs"> Import Nhân sự</span></button>
<div id="CustomersImport" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close closeModalImport" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title">Danh Sách Import</h4>
            </div>
            <div class="modal-body">
                <table id="tblCustomersImport" class="table" data-toolbar="#toolbar"></table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger closeModalImport">Đóng</button>
                <button type="submit" class="btn btn-success" id="btnAddCustomersImport">Lưu</button>
            </div>
        </div>
    </div>
</div>